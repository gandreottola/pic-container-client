{{#each images as |image|}}
  {{#if image.editable}}
  {{log image.owner}}
  <img src="{{image.url}}"/>
  <h3>Date Created: {{image.createdAt}}</h3>
  <h3>Date Modified: {{image.updatedAt}}</h3>
  <h3>Owner: {{image.userName}}</h3>
  <h3>Tags</h3>
  <ul>
  {{#each tag as |tag|}}
      <li>{{tag}}</li>
  {{/each}}
</ul>
  <button class="delete-image btn btn-danger delete col-mb-3" data-del-image="{{image._id}}">Delete Image</button>

  <br>
  {{/if}}
{{/each}}
